{
  "version": "1.0.0",
  "protocol": {
    "message_format": "json",
    "encoding": "utf-8",
    "max_message_size": "10MB",
    "timeout": 300000
  },
  "agent_communication": {
    "request": {
      "agent": "string",
      "task": "string",
      "context": "object",
      "priority": "number",
      "timeout": "number"
    },
    "response": {
      "agent": "string",
      "status": "success|error|timeout",
      "result": "any",
      "error": "string",
      "duration": "number"
    }
  },
  "parallel_execution": {
    "max_concurrent": 10,
    "queue_size": 100,
    "priority_levels": 3
  },
  "error_handling": {
    "retry_count": 3,
    "retry_delay": 1000,
    "fallback_agent": "orchestrator"
  }
}
